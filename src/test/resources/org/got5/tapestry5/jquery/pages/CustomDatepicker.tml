<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">


<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>CustomDatePicker</h1>
			</div>
			<form t:type="form" t:id="idForm">
		    	<t:label t:for="dateField">The Date : </t:label>
		      	<t:datefield t:id="dateField" t:value="value" t:mixins="jquery/CustomDatepicker" t:params="params"/>
		    </form>
		</section>
	</div>
</div>
</html>