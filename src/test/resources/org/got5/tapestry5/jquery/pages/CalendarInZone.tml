<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">


<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>DateField In Zone</h1>
			</div>
			<t:zone t:id="zone" t:visible="false">
				
					<t:form t:zone="zone">
						<t:datefield t:value="date" t:mixins="customdatepicker"/>
						<t:submit />
					</t:form>
				
			</t:zone>

			<t:actionLink t:id="link" t:zone="zone">Link</t:actionLink>
			<p>

				<t:actionlink t:id="english"> english</t:actionlink>
				<br />
				<t:actionlink t:id="french" t:zone="zone"> french</t:actionlink>
				<br />

			</p>
		</section>
	</div>
</div>
</html>