<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
	xmlns:p="tapestry:parameter">


<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>Bind</h1>
			</div>
			<t:zone t:id="clickZone" t:visible="false">
   		 		${message}
   		 	</t:zone>

			<a t:id="clickHere" t:type="any" t:mixins="jquery/bind"
               bind.eventType="click" bind.event="click" bind.zone="${clickzone.clientid}">click
				test</a>
		</section>
	</div>
</div>
</html>