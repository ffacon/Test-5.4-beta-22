<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">


<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>DateField</h1>
			</div>
			<t:form>
				<t:datefield t:value="date" t:format="MM/dd/yy"/>
				<t:submit />
			</t:form>
			<p>Date value is : ${date}</p>
		</section>
	</div>
</div>
</html>