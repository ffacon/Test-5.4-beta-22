<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">

<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>Checkbox</h1>
			</div>
			<t:form t:id="monForm">

				<fieldset>
					<t:label t:for="maBox">Phone Number : </t:label>
					<input t:type="jquery/checkbox" t:id="maBox" t:value="value" />

				</fieldset>

			</t:form>
		</section>
	</div>
</div>
</html>