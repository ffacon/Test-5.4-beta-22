<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">


<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>DDSlick</h1>
			</div>
			<t:zone t:id="myZone" id="myZone">

				<t:loop source="options" value="currentOptions" t:index="index">
					<t:jquery.DdSlick t:params="currentOptions"
						t:event="onSelectedElement" t:zone="myZone" />
				</t:loop>

			${value}
		</t:zone>
		</section>
	</div>
</div>
</html>