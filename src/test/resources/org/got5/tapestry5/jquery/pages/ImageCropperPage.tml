<html t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">


<div class="row">
	<div class="span12">
		<section id="list" style="padding-top: 30px;">
			<div class="page-header">
				<h1>Image Cropper</h1>
			</div>
			<img t:type="jquery/ImageCropper" src="img/gallery2.jpg"
				alt="ma cabane au canada" border="0" t:zone="myImageCropped"
				t:id="myImageCropper" t:options="options" />
			<t:zone t:id="myImageCropped" id="myImageCropped">
			The crop parameters x=${x} y=${y} x2=${x2} y2=${y2} w=${w} h=${h}.
		</t:zone>
		</section>
	</div>
</div>
</html>